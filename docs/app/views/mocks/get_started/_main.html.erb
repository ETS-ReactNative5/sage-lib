<div class="sage-tabs-container">
  <%= sage_component SageTabs, {
    id: "example-tabs1",
    items: [
      {
        text: "Dashboard module",
        target: "feature-1",
        active: true
      },
      {
        text: "Sidebar module",
        target: "feature-2",
      },
      {
        text: "App tour modal",
        target: "feature-3",
      },
    ]
  } %>
  <%= sage_component SageTabsPane, { id: "feature-1" } do %>
    <%= render "mocks/get_started/dashboard_module" %>
  <% end %>
  <%= sage_component SageTabsPane, { id: "feature-2" } do %>
    <%= render "mocks/get_started/sidebar_module" %>
  <% end %>
  <%= sage_component SageTabsPane, { id: "feature-3" } do %>
    <%= render "mocks/get_started/app_tour_module" %>
  <% end %>
</div>
