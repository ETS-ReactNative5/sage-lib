    <%= sage_layout SageFrame, { gap: "md" } do %>
      <%= sage_layout SageFrame, {
        background: "#f5bc60",
        border_radius: "md",
        padding: "md",
        width: "fill",
      } do %>
        <%# TODO: Need to add video player with large play button overlay %>
        [video player]
      <% end %>
      <%= sage_layout SageFrame, { gap: "xs", width: "400px" } do %>
        <h3 class="<%= "#{SageClassnames::TYPE::HEADING_4}" %>">
          All about payouts
        </h3>
        <p class="<%= "#{SageClassnames::TYPE::BODY_XSMALL} #{SageClassnames::TYPE_COLORS::CHARCOAL_400}" %>">
          Offers are a powerful way to bundle and sell your products.
          In this short video we&rsquo;ll cover all the basics.
        </p>
      <% end %>
      <%= sage_layout SageFrame, {
        align: "center-spread",
        direction: "horizontal",
        gap: "xs",
        width: "fill",
      } do %>
        <%= sage_component SageButton, {
          value: "Create your first offer",
          style: "primary",
          attributes: {
            href: "#TODO-dev-create-offer-url",
          },
        } %>
        <%= sage_component SageButton, {
          value: "Learn more",
          style: "primary",
          subtle: true,
          attributes: {
            href: "#TODO-dev-offers-learn-more-url",
          },
        } %>
      <% end %>
    <% end %>
  <% end %>
<% end %>
