<%= javascript_include_tag "//cdn.jsdelivr.net/gh/google/code-prettify@master/loader/run_prettify.js", cache: true %>
<% if SageRails.next_theme? %>
  <%= javascript_pack_tag "docs-next" %>
<% else %>
  <%= javascript_pack_tag "docs-legacy" %>
<% end %>


<script type="text/javascript">
  window.addEventListener("DOMContentLoaded", function(){
    Sage.init([
      'table',
      'tooltip',
      'sidebar',
      'overlay',
      'alert',
      'banner',
      'select',
      'meter',
      'inputaffixes',
      'inputgroup',
      'modal',
      'dropdown',
      'sortable',
      'accordion',
    ]);
  });
</script>
