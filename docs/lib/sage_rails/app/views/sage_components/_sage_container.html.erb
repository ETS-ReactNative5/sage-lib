<%
size = component.size.present? ? component.size : "full"
tag = component.tag ? component.tag : "div"
%>
<<%= tag %> class="
    sage-container
    sage-container--<%= size %>
    <%= component.generated_css_classes %>
  "
  <%= component.generated_html_attributes.html_safe %>
>
  <%= component.content %>
</<%= tag %>>
